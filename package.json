{
  "name": "calend",
  "version": "0.1.0",
  "author": "nibdo",
  "license": "MIT",
  "private": false,
  "declaration": true,
  "source": "src/index.tsx",
  "main": "dist/main.js",
  "types": "dist/index.d.ts",
  "module": "dist/module.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/nibdo/calend"
  },
  "dependencies": {
    "@swc/helpers": "^0.2.14",
    "luxon": "2.1.1",
    "regenerator-runtime": "^0.13.9"
  },
  "devDependencies": {
    "@parcel/packager-ts": "^2.0.1",
    "@parcel/transformer-sass": "^2.0.1",
    "@parcel/transformer-typescript-types": "^2.0.1",
    "@types/jest": "27.0.3",
    "@types/luxon": "2.0.7",
    "@types/mocha": "9.0.0",
    "@types/node": "14.17.34",
    "@types/react": "17.0.35",
    "@types/react-dom": "17.0.11",
    "@typescript-eslint/eslint-plugin": "2.34.0",
    "@typescript-eslint/parser": "5.4.0",
    "chai": "4.3.4",
    "copyfiles": "2.4.1",
    "eslint-config-prettier": "5.1.0",
    "eslint-config-react": "1.1.7",
    "eslint-plugin-prettier": "4.0.0",
    "gulp": "4.0.2",
    "gulp-concat": "2.6.1",
    "mocha": "9.1.3",
    "node-sass": "4.14.1",
    "parcel": "^2.0.1",
    "prettier": "2.4.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-scripts": "4.0.3",
    "ts-mocha": "8.0.0",
    "tsconfig-paths": "3.12.0",
    "typescript": "4.5.2"
  },
  "peerDependencies": {
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "scripts": {
    "start": "BROWSER=none PORT=4103 react-scripts start",
    "build": "parcel build",
    "watch": "parcel watch",
    "dev": "touch index.css && cp index.css ./dist/styles && npm run compile && npm run copy && npm run compile-scss && gulp && rm -r ./dist/styles/tmp",
    "compile": "tsc --module commonjs",
    "compile-scss": "node-sass ./src/ --output ./dist/styles/tmp",
    "copy-style": "copyfiles -u 1 \"./src/**/*.scss\" \"dist\"",
    "copy": "cp ./package.json ./dist/ && cp ./LICENSE ./dist/ && cp ./README.md ./dist/",
    "test": "ts-mocha --config ./mocharc.js"
  },
  "keywords": [
    "javascript"
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
